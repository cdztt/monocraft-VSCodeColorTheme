  /*****************************************************************************
   * priest: 		    #ccc8c8		 9.0        variable
   * priest: 		    #868383		 4.0        operator and bracket
   *****************************************************************************
   * mage:		      #7bc9df		 8.0        function
   *****************************************************************************
   * rogue:         #989040    4.5        keyword and storage
   *****************************************************************************
   * paladin:       #ff1c7e		 4.0        cursor
   *****************************************************************************
   * warrior:       #b68f65		 5.0        string literal
   *****************************************************************************
   * shaman:	      #3e7ebd		 3.5        type and class
   *****************************************************************************
   * hunter:			  #7fbd34		 6.5			  html tag
   *****************************************************************************
   * monk:		      #20bc7b		 6.0        component
   *****************************************************************************
   * warlock:		    #7373b9		 3.5        Tag attribute
   *****************************************************************************
   * druid:	        #e9842b		 5.5        number literal
   * druid:	        #a26025		 3.0        constant
   *****************************************************************************
   * demonhunter:   #7d7a7f		 3.5        comment
   *****************************************************************************
   * deathknight:   #9a2d3f		 2.0        selection color
   *****************************************************************************
   * evoker:        #4fae94    5.5        selection color
   ****************************************************************************/

  "tokenColors": [
    {
      "name": "Comment and semicolon",
      "scope": [
        "comment",
        "punctuation.definition.block.tag",
        "punctuation.definition.bracket.curly",
        "punctuation.definition.bracket",
        "punctuation.definition.comment",
        "storage.type.class.jsdoc",
        "entity.name.type.instance.jsdoc",
        "variable.other.jsdoc",
        "punctuation.terminator.statement"
      ],
      "settings": {
        "foreground": "#7d7a7f"
      }
    },
    {
      "name": "Variable",
      "scope": [
        "source",
        "source.css",
        "variable.other",
        "variable.language",
        "string.unquoted",
        "meta.embedded",
        "meta.brace",
        "meta.delimiter",
        "meta.property-value.css",
        "punctuation.separator",
        "punctuation.terminator",
        "punctuation.destructuring",
        "text.html.markdown",
        "text.html.derivative",
        "JSXNested",
        "text.html.vue-html",
        "support.type.property-name"
      ],
      "settings": {
        "foreground": "#ccc8c8"
      }
    },
    {
      "name": "Tag attribute",
      "scope": [
        "entity.other.attribute-name",
        "punctuation.attribute-shorthand",
        "punctuation.section.embedded",
        "punctuation.definition.interpolation.begin.html",
        "punctuation.definition.interpolation.end.html"
      ],
      "settings": {
        "foreground": "#7373b9"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "punctuation.definition.string",
        "punctuation.definition.quasi",
        "string.regexp.js punctuation.definition.string",
        "string.quoted.double.json"
      ],
      "settings": {
        "foreground": "#b68f65"
      }
    },
    {
      "name": "storage",
      "scope": [
        "storage.type",
        "keyword",
        "keyword.other",
        "keyword.control.rust",
        "keyword.operator.borrow.and.rust",
        "keyword.operator.new",
        "storage.type.rust",
        "storage.modifier",
        "meta.brace.square",
        "punctuation.brackets.square.rust",
        "punctuation.definition.binding-pattern.array"
      ],
      "settings": {
        "foreground": "#989040"
      }
    },
    {
      "name": "operator",
      "scope": [
        "keyword.operator",
        "punctuation.brackets.curly.rust",
        "punctuation.brackets.round.rust",
        "punctuation.brackets.angle.rust",
        "punctuation.section",
        "punctuation.bracket",
        "punctuation.definition.parameters",
        "punctuation.definition.template-expression",
        "punctuation.accessor",
        "meta.brace.curly",
        "meta.brace.round",
        "meta.function.js",
        "entity.quasi.element.js",
        "punctuation.definition.arguments",
        "punctuation.parenthesis",
        "punctuation.definition.begin.bracket",
        "punctuation.definition.end.bracket",
        "meta.function.arrow.js",
        "meta.method-call.with-arguments.js",
        "punctuation.separator.key-value",
        "meta.delimiter.comma.js",
        "punctuation.terminator.rule.css",
        "punctuation.definition.tag",
        "punctuation.definition.entity.css",
        "punctuation.definition.block",
        "meta.method-call.without-arguments",
        "punctuation.definition.interpolation",
        "punctuation.quasi.element",
        "punctuation.definition.typeparameters"
      ],
      "settings": {
        "foreground": "#868383"
      }
    },
    {
      "name": "Number",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#e9842b"
      }
    },
    {
      "name": "Built-in constant",
      "scope": ["constant.language", "support.constant"],
      "settings": {
        "foreground": "#a26025"
      }
    },
    {
      "name": "User-defined constant",
      "scope": "constant.character, constant.other",
      "settings": {
        "foreground": "#a26025"
      }
    },
    {
      "name": "Class name",
      "scope": [
        "support.type",
        "entity.name.type",
        "entity.name.class",
        "entity.name.namespace",
        "entity.name.scope-resolution",
        "variable.language.this"
      ],
      "settings": {
        "foreground": "#3e7ebd"
      }
    },
    {
      "name": "Function name",
      "scope": ["entity.name.function", "meta.function-call.generic"],
      "settings": {
        "foreground": "#7bc9df"
      }
    },
    {
      "name": "Library function",
      "scope": "support.function",
      "settings": {
        "foreground": "#7bc9df"
      }
    },
    {
      "name": "Tag name",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#7fbd34"
      }
    },
    {
      "name": "component",
      "scope": "support.class.component",
      "settings": {
        "foreground": "#20bc7b"
      }
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {
        "foreground": "#F44747"
      }
    },
    {
      "name": "Invalid deprecated",
      "scope": "invalid.deprecated",
      "settings": {
        "foreground": "#F44747"
      }
    },
    {
      "name": "diff.header",
      "scope": "meta.diff, meta.diff.header",
      "settings": {
        "foreground": "#75715E"
      }
    },
    {
      "name": "diff.deleted",
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#dd83d5"
      }
    },
    {
      "name": "diff.inserted",
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#A6E22E"
      }
    },
    {
      "name": "diff.changed",
      "scope": "markup.changed",
      "settings": {
        "foreground": "#e9842b"
      }
    },
    {
      "scope": "constant.numeric.line-number.find-in-files - match",
      "settings": {
        "foreground": "#7373b9A0"
      }
    },
    {
      "scope": "entity.name.filename.find-in-files",
      "settings": {
        "foreground": "#e9842b"
      }
    },
    {
      "name": "Markup Quote",
      "scope": "markup.quote",
      "settings": {
        "foreground": "#dd83d5"
      }
    },
    {
      "name": "Markup Lists",
      "scope": "markup.list",
      "settings": {
        "foreground": "#e9842b"
      }
    },
    {
      "name": "Markup Styling",
      "scope": "markup.bold, markup.italic",
      "settings": {
        "foreground": "#66D9EF"
      }
    },
    {
      "name": "Markup Inline",
      "scope": "markup.inline.raw",
      "settings": {
        "foreground": "#e9842b"
      }
    },
    {
      "name": "Markup Headings, markdown Header",
      "scope": "markup.heading",
      "settings": {
        "foreground": "#A6E22E"
      }
    },
    {
      "name": "Markup Setext Header",
      "scope": "markup.heading.setext",
      "settings": {
        "foreground": "#A6E22E",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Headings",
      "scope": "markup.heading.markdown",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Quote",
      "scope": "markup.quote.markdown",
      "settings": {
        "foreground": "#e2e22e"
      }
    },
    {
      "name": "Markdown Bold",
      "scope": "markup.bold.markdown",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Link Title/Description",
      "scope": [
        "string.other.link.title.markdown",
        "string.other.link.description.markdown"
      ],
      "settings": {
        "foreground": "#AE81FF"
      }
    },
    {
      "name": "Markdown Underline Link/Image",
      "scope": "markup.underline.link.markdown, markup.underline.link.image.markdown",
      "settings": {
        "foreground": "#e9842b"
      }
    },
    {
      "name": "Markdown Emphasis",
      "scope": "markup.italic.markdown",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown Punctuation Definition Link",
      "scope": "markup.list.unnumbered.markdown, markup.list.numbered.markdown",
      "settings": {
        "foreground": "#ccc8c8"
      }
    },
    {
      "name": "Markdown List Punctuation",
      "scope": ["punctuation.definition.list.begin.markdown"],
      "settings": {
        "foreground": "#A6E22E"
      }
    },
    {
      "scope": "token.info-token",
      "settings": {
        "foreground": "#6796e6"
      }
    },
    {
      "scope": "token.warn-token",
      "settings": {
        "foreground": "#cd9731"
      }
    },
    {
      "scope": "token.error-token",
      "settings": {
        "foreground": "#f44747"
      }
    },
    {
      "scope": "token.debug-token",
      "settings": {
        "foreground": "#b267e6"
      }
    },
    {
      "name": "markdown code",
      "scope": ["markup.fenced_code.block.markdown"],
      "settings": {
        "foreground": "#7fbd34"
      }
    }
  ],
  "semanticHighlighting": true
}
