	/*
	priest: 		#e4e2e2		11.5		variant and tag attribution
	mage: 			#8be6ff		10.5		function
	rogue:			#e4d12e		9.5			keyword
    rogue-stealth:  #a3982f     5.0         type
	paladin:		#ffa3cb		8.0         operator and interpolation
	warrior:		#C0966A		5.5			string literal
	shaman:			#53a9ff		6.0			class
	hunter:			#7FC524		7.0			html tag
	monk:			#00bd6e		6.0			support.class
	druid:			#ff8317		6.0         number literal
	warlock:		#9393bb		5.0         this
	deathknight:	#FF6D85		5.5         constant
    /*deprecated
	demonhunter:	#c27ada		5.0
	*/
	"tokenColors": [
		{
			"scope": [
				"meta.embedded",
				"source.groovy.embedded"
			],
			"settings": {
				"foreground": "#e4e2e2"
			}
		},
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.block.tag",
				"punctuation.definition.bracket.curly",
				"punctuation.definition.bracket",
				"punctuation.definition.comment",
				"storage.type.class.jsdoc",
				"entity.name.type.instance.jsdoc",
				"variable.other.jsdoc"
			],
			"settings": {
				"foreground": "#88846f"
			}
		},
		{
			"name": "String",
			"scope": [
				"string",
				"punctuation.definition.string"
			],
			"settings": {
				"foreground": "#C0966A"
			}
		},
		{
			"name": "Template Definition and interpolation",
			"scope": [
				"punctuation.definition.template-expression",
				"punctuation.section.embedded",
                "punctuation.quasi.element",
                "punctuation.definition.quasi"
			],
			"settings": {
				"foreground": "#ffa3cb"
			}
		},
		{
			"name": "Number",
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#ff8317"
			}
		},
		{
			"name": "Built-in constant",
			"scope": "constant.language",
			"settings": {
				"foreground": "#ff8317"
			}
		},
		{
			"name": "User-defined constant",
			"scope": "constant.character, constant.other",
			"settings": {
				"foreground": "#FF6D85"
			}
		},
		{
			"name": "Variable",
			"scope": [
                "variable.other",
                "string.unquoted"
			],
			"settings": {
				"foreground": "#e4e2e2"
			}
		},
		{
			"name": "Keyword",
			"scope": [
                "keyword",
                "storage.type.function.arrow"
            ],
			"settings": {
				"foreground": "#e4d12e"
			}
		},
		{
			"name": "modifier",
			"scope": "storage.modifier",
			"settings": {
				"foreground": "#e4d12e"
			}
		},
		{
			"name": "Storage type",
			"scope": "storage.type",
			"settings": {
				"foreground": "#a3982f"
			}
		},
        {
			"name": "operator",
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#ffa3cb",
			}
		},
        {
			"name": "assignment",
			"scope": [
                "keyword.operator.assignment",
                "keyword.operator.comparison",
                "keyword.operator.relational",
            ],
			"settings": {
				"foreground": "#ffa3cb",
                "fontStyle": "bold"
			}
		},
		{
			"name": "Class name",
			"scope": [
				"entity.name.type",
				"entity.name.class",
				"entity.name.namespace",
				"entity.name.scope-resolution",
				"support.type",
			],
			"settings": {
				"foreground": "#53a9ff"
			}
		},
		{
			"name": "Function name",
			"scope": "entity.name.function",
			"settings": {
				"foreground": "#8be6ff"
			}
		},
		{
			"name": "this",
			"scope": "variable.language",
			"settings": {
				"foreground": "#9393bb",
                "fontStyle": "italic"
			}
		},
		{
			"name": "Tag name",
			"scope": "entity.name.tag",
			"settings": {
				"foreground": "#7FC524"
			}
		},
		{
			"name": "Tag attribute",
			"scope": "entity.other.attribute-name",
			"settings": {
				"foreground": "#e4e2e2"
			}
		},
		{
			"name": "Library function",
			"scope": "support.function",
			"settings": {
				"foreground": "#8be6ff"
			}
		},
		{
			"name": "Library constant",
			"scope": [
				"support.constant",
			],
			"settings": {
				"foreground": "#53A9FF"
			}
		},
		{
			"name": "Library class",
			"scope": [
				"support.class",
			],
			"settings": {
				"foreground": "#00bd6e"
			}
		},
		{
			"name": "Invalid",
			"scope": "invalid",
			"settings": {
				"foreground": "#F44747"
			}
		},
		{
			"name": "Invalid deprecated",
			"scope": "invalid.deprecated",
			"settings": {
				"foreground": "#F44747"
			}
		},
		{
			"name": "JSON String",
			"scope": [
				"meta.structure.dictionary.json",
				"string.quoted.double.json"
			],
			"settings": {
				"foreground": "#e4e2e2"
			}
		},
		{
			"name": "diff.header",
			"scope": "meta.diff, meta.diff.header",
			"settings": {
				"foreground": "#75715E"
			}
		},
		{
			"name": "diff.deleted",
			"scope": "markup.deleted",
			"settings": {
				"foreground": "#dd83d5"
			}
		},
		{
			"name": "diff.inserted",
			"scope": "markup.inserted",
			"settings": {
				"foreground": "#A6E22E"
			}
		},
		{
			"name": "diff.changed",
			"scope": "markup.changed",
			"settings": {
				"foreground": "#ff8317"
			}
		},
		{
			"scope": "constant.numeric.line-number.find-in-files - match",
			"settings": {
				"foreground": "#9393bbA0"
			}
		},
		{
			"scope": "entity.name.filename.find-in-files",
			"settings": {
				"foreground": "#ff8317"
			}
		},
		{
			"name": "Markup Quote",
			"scope": "markup.quote",
			"settings": {
				"foreground": "#dd83d5"
			}
		},
		{
			"name": "Markup Lists",
			"scope": "markup.list",
			"settings": {
				"foreground": "#ff8317"
			}
		},
		{
			"name": "Markup Styling",
			"scope": "markup.bold, markup.italic",
			"settings": {
				"foreground": "#66D9EF"
			}
		},
		{
			"name": "Markup Inline",
			"scope": "markup.inline.raw",
			"settings": {
				"foreground": "#ff8317"
			}
		},
		{
			"name": "Markup Headings, markdown Header",
			"scope": "markup.heading",
			"settings": {
				"foreground": "#A6E22E"
			}
		},
		{
			"name": "Markup Setext Header",
			"scope": "markup.heading.setext",
			"settings": {
				"foreground": "#A6E22E",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markup Headings",
			"scope": "markup.heading.markdown",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markdown Quote",
			"scope": "markup.quote.markdown",
			"settings": {
				"foreground": "#e2e22e"
			}
		},
		{
			"name": "Markdown Bold",
			"scope": "markup.bold.markdown",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markdown Link Title/Description",
			"scope": [
				"string.other.link.title.markdown",
				"string.other.link.description.markdown"
			],
			"settings": {
				"foreground": "#AE81FF"
			}
		},
		{
			"name": "Markdown Underline Link/Image",
			"scope": "markup.underline.link.markdown,markup.underline.link.image.markdown",
			"settings": {
				"foreground": "#ff8317"
			}
		},
		{
			"name": "Markdown Emphasis",
			"scope": "markup.italic.markdown",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"name": "Markdown Punctuation Definition Link",
			"scope": "markup.list.unnumbered.markdown, markup.list.numbered.markdown",
			"settings": {
				"foreground": "#e4e2e2"
			}
		},
		{
			"name": "Markdown List Punctuation",
			"scope": [
				"punctuation.definition.list.begin.markdown"
			],
			"settings": {
				"foreground": "#A6E22E"
			}
		},
		{
			"scope": "token.info-token",
			"settings": {
				"foreground": "#6796e6"
			}
		},
		{
			"scope": "token.warn-token",
			"settings": {
				"foreground": "#cd9731"
			}
		},
		{
			"scope": "token.error-token",
			"settings": {
				"foreground": "#f44747"
			}
		},
		{
			"scope": "token.debug-token",
			"settings": {
				"foreground": "#b267e6"
			}
		},
		{
			"name": "punctuation",
			"scope": [
				"meta.property-value.css",
				"source.css",
				"punctuation.section.property-list",
				"punctuation.separator",
				"punctuation.terminator",
				"punctuation.definition",
				"punctuation.accessor",
				"meta.brace",
				"text.html.markdown",
				"text.html.derivative",
				"JSXNested",
                "text.html.vue-html",
                "meta.delimiter.comma.js",
			],
			"settings": {
				"foreground": "#e4e2e2"
			}
		},
		{
			"name": "markdown code",
			"scope": [
				"markup.fenced_code.block.markdown"
			],
			"settings": {
				"foreground": "#00bd6e"
			}
		},
	],
	//"semanticHighlighting": true
}
