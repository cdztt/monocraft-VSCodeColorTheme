{
  "name": "monocraft",
  "publisher": "cdztt",
  "displayName": "MonoCraft",
  "description": "Dark color theme and Chinese / English translator",
  "version": "1.9.27",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cdztt/monocraft-VSCodeColorTheme.git"
  },
  "license": "MIT",
  "main": "./build/index.js",
  "scripts": {
    "tscw": "tsc --watch",
    "tsc": "tsc",
    "test": "jest",
    "wp:build": "webpack --mode production",
    "vsce:pack": "vsce package",
    "vsce:pub": "vsce publish"
  },
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Themes",
    "Keymaps",
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "themes": [
      {
        "label": "MonoCraft",
        "uiTheme": "vs-dark",
        "path": "./themes/MonoCraft-color-theme.json"
      }
    ],
    "keybindings": [
      {
        "key": "ctrl+,",
        "command": "edit.appendComma",
        "when": "editorTextFocus && !editorReadonly"
      },
      {
        "key": "ctrl+;",
        "command": "edit.appendSemicolon",
        "when": "editorTextFocus && !editorReadonly"
      },
      {
        "key": "ctrl+k ctrl+;",
        "command": "edit.insertBlock",
        "when": "editorTextFocus && !editorReadonly"
      },
      {
        "key": "ctrl+.",
        "command": "edit.insertArrow",
        "when": "editorTextFocus && !editorReadonly"
      },
      {
        "key": "shift+space",
        "command": "edit.insertSpaceBehind",
        "when": "editorTextFocus && !editorReadonly"
      },
      {
        "key": "ctrl+'",
        "command": "tran.sele",
        "when": "editorTextFocus && !editorReadonly"
      },
      {
        "key": "ctrl+shift+'",
        "command": "tran.seleToEn",
        "when": "editorTextFocus && !editorReadonly"
      }
    ],
    "commands": [
      {
        "command": "tran.sele",
        "title": "tranSele"
      },
      {
        "command": "tran.seleToEn",
        "title": "tranSeleToEn"
      },
      {
        "command": "tran.all",
        "title": "tranAll"
      },
      {
        "command": "color.red",
        "title": "colorRed"
      },
      {
        "command": "color.green",
        "title": "colorGreen"
      },
      {
        "command": "color.blue",
        "title": "colorBlue"
      },
      {
        "command": "color.coral",
        "title": "colorCoral"
      },
      {
        "command": "color.auto.red",
        "title": "colorAutoRed"
      },
      {
        "command": "color.auto.green",
        "title": "colorAutoGreen"
      },
      {
        "command": "color.auto.blue",
        "title": "colorAutoBlue"
      },
      {
        "command": "color.auto.coral",
        "title": "colorAutoCoral"
      }
    ]
  },
  "dependencies": {
    "axios": "^1.4.0",
    "isomorphic-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "@cdztt/gulp-trimcomments": "^1.0.8",
    "@types/isomorphic-fetch": "^0.0.36",
    "@types/node": "^20.4.2",
    "@types/vscode": "^1.80.0",
    "@typescript-eslint/parser": "^6.0.0",
    "babel-loader": "^9.1.3",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "gulp": "^4.0.2",
    "jest": "^29.6.1",
    "typescript": "^5.1.6",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0"
  }
}