{
  "name": "monocraft",
  "displayName": "MonoCraft",
  "description": "a dark color theme and some useful keybindings",
  "version": "1.3.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cdztt/monocraft-VSCodeColorTheme.git"
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.56.0"
  },
  "categories": [
    "Themes",
    "Other"
  ],
  "activationEvents": [
    "onCommand:editor.action.appendComma",
    "onCommand:editor.action.appendSemicolon"
  ],
  "main": "./keybindings/index.js",
  "contributes": {
    "themes": [
      {
        "label": "MonoCraft",
        "uiTheme": "vs-dark",
        "path": "./themes/MonoCraft-color-theme.json"
      }
    ],
    "keybindings": [
      {
        "key": "ctrl+,",
        "command": "editor.action.appendComma",
        "when": "editorTextFocus && !editorReadonly"
      },
      {
        "key": "ctrl+;",
        "command": "editor.action.appendSemicolon",
        "when": "editorTextFocus && !editorReadonly"
      },
      {
        "key": "ctrl+'",
        "command": "tran.sele",
        "when": "editorTextFocus && !editorReadonly"
      }
    ],
    "commands": [
      {
        "command": "tran.sele",
        "title": "tranSele"
      },
      {
        "command": "tran.all",
        "title": "tranAll"
      }
    ]
  },
  "dependencies": {
    "axios": "^1.4.0"
  }
}